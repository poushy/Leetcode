/**
 * @param {number} numRows
 * @return {number[][]}
 */
var generate = function(numRows) {
    if (!numRows) {
        return [];
    }
    const arr = [[1]];
    for(let i = 1; i< numRows; i++) {
        const row = [1];
        for(let j = 1; j < i; j++ ) {
            row.push(arr[i-1][j-1] + arr[i-1][j]);
        }
        row.push(1);
        arr.push(row);
    }
    return arr;
};
